import java.util.*;
class Removingvowels {
    public static void main(String[] args) {
        Scanner in=new Scanner(System.in);
        String s=in.next();
        String t="";
        for(int i=0;i<s.length();i++){
            char ch=s.charAt(i);
            if(!(ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u' || ch == 'A' || ch == 'E' || ch == 'I' || ch =='O' || ch == 'U')){
                t=t+ch;
            }
        }
        System.out.println(t);
    }
}
